<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>get CRUD</title>
    <style>
        table, th, td {
          border:1px solid black;
        }
        </style>
</head>
<body>
    <table style="width:100%">
        <tr>
          <td>Firstname</td>
          <td>Lastname</td>
          <td>Email</td>
          <td>Address</td>
          <td>Role</td>
          <td></td>
        </tr>


        <% for (let i = 0; i < dataTable.length; i++ ) {%>
            <tr>
                <td><%= dataTable[i].firstName %></td>
                <td><%= dataTable[i].lastName %></td>
                <td><%= dataTable[i].email %></td>
                <td><%= dataTable[i].address %></td>
                <td><%= dataTable[i].role %></td>
                <td>

                    <a href="/edit-crud?id=<%= dataTable[i].id%>" type="button">Edit</button>
                    <button type="button">Delete</button>

                </td>

            </tr>
        <% } %>
      </table>
      

</body>
</html>